<body>
  <h1>Conversation with {{muName}}</h1>
  <ul id="messageslist">
    {{#each messages}}
    <li> {{createdAt}} </li>
    <li> {{senderName}}: {{content}}</li>
    
    {{/each}}
  </ul>
  <form action="/messages/{{userId}}/messages/{{matchUserId}}/send" method="post" id="form10">
    <input type="text" name="content" placeholder="Type your message..." required>
    <input type="submit" value="Send" >
  </form>
<script src="https://code.jquery.com/jquery-2.2.4.min.js"
